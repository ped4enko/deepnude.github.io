  <script>


            var post_max_size = '10';
            var upload_max_filesize = '1600';
            var wp_memory_limit = '40';
  </script> 
  <script>


        $(document).ready(function(){
                $(".learnmore").addClass("content-button");

                $(".content-button").each(function(){

                        if ($(this).children("span").length == 0)
                        {
                                var txt = $(this).html();
                                $(this).html("");
                                $(this).append("<span>"+txt+"</span>")
                        }

                })

        })
  </script>
