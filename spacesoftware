                        function switchBanners()
                        {
                                //alert("in")
                                current = $('.bg.current');
                                next = $(current).next();
                                if (!(next.length))
                                        next = $('.bg:first')

                                current.removeClass('current');
                                next.addClass('current').addClass('added');
                        }
                        var menuheight;
                        function setMenuHeight()
                        {
                                if ($(window).width() < 650)
                                        menuheight = 60;
                                else
                                        menuheight = 40;
                                $('.menu-wrap').removeClass("vis");
                        }
                        function resizeElements()
                        {
                                $('.hovertext').each(function(){
                                        he = $(this).find('div').height();
                                        wid = $(this).find('div').width();
                                        $(this).css("height", he).css("width", wid);
                                })
                        }
                        function getHelpFile(id)
                        {

                        }
                        function ShouldIStayOrShouldIGo(itm)
                        {
                                //console.log($('body').width())
                                $(".menu-item").not(itm).data('clicked', 0);
                                if (  (navigator.userAgent.match(/(iPod|iPhone|iPad)/) || navigator.userAgent.toLowerCase().indexOf("android") > -1 ) && $('body').width() > 700 ) {

                                        if (itm.parent().children("div").length>0)
                                        {
                                                if (itm.data('clicked') == 1)
                                                {
                                                        window.location = itm.attr("href");
                                                }
                                                else
                                                {
                                                        itm.data('clicked',1)
                                                }
                                        }
                                        else
                                                window.location = itm.attr("href");
                                }
                                else
                                        window.location = itm.attr("href");
                        }
                        $(document).ready(function(){
                                setMenuHeight();
                                setInterval(switchBanners, 8000)
                                $("a.video, a.picture, a.inline-video").fancybox();
                                window.resizeBy(1,0);
                                if ($('.btn-ffvc').length>0)
                                {
                                        $('.btn-ffvc').on('click', function(){
                                                if ($('.ffvc').css('display') == 'block')
                                                        $('.ffvc').slideUp(500);
                                                else
                                                        $('.ffvc').slideDown(500);
                                        })
                                        $('.btn-collapse').on('click', function(){
                                                $('.ffvc').slideUp(500);
                                        })
                                }
                                $(".scrollbar").customScrollbar({
                                        updateOnWindowResize: "false"
                                });
                                //      blocks clicking while the animation is playing
                                var ivAnimating = false;
                                $('#img-left, #img-right, #vid-left, #vid-right').on("click", function(){
                                        //      if not already animating
                                        if (ivAnimating == false)
                                        {
                                                //      lock the buttons
                                                ivAnimating = true;
                                                //      get the id
                                                var id = $(this).attr("id");
                                                //      figure out if it is images or videos
                                                if (id == "img-left" || id == "img-right")
                                                        el = "images"
                                                else
                                                        el = "videos"
                                                //      get the wrapper info
                                                var wrap = $('.'+el+'list')
                                                var wrapwidth = wrap.css("width").replace("px", "");
                                                //      figure out which way the slides need to move
                                                if (id == "img-left" || id == "vid-left")
                                                        dir = "1"
                                                else
                                                        dir = "-1"
                                                //      set the element that is scrolling
                                                var toScroll = $('.'+el);
                                                //      get the left margin of the element.  this is what is adjusted to slide the list around
                                                var left = toScroll.css("margin-left").replace("px","");
                                                //      the width of the scroll element
                                                var max = toScroll.css("width").replace("px", "")
                                                //      handles the left moves
                                                if ((left <=0 )&& (parseInt((-1 * wrapwidth) + parseInt(left)) >= parseInt(-1 * max)) && dir == "-1")
                                                        toScroll.animate({"margin-left" : parseInt(left) + parseInt(dir * (200)) }, 500, function(){ivAnimating = false;})
                                                //      handles the right moves
                                                if (left < 0 && dir == "1")
                                                        toScroll.animate({"margin-left" : parseInt(left) + parseInt(dir * (200)) }, 500, function(){ivAnimating = false;})
                                        }
                                })

                                //$("a.iframe").fancybox();

                                $('.icon-navicon').on('click', function(){

                                        $('.menu-wrap').toggleClass("vis");

                                })
                                if ($(".hide-timer").length>0)
                                        setTimeout(function(){$(".hide-timer").slideUp(400)}, 5000)
                        });

                        /*$(window).load(function(){*/
      $( document ).ready(function() {
                                if ($('.tabs'))
                                {
                                        $('.tabs-sidebar a').on('click', function(){
                                                $('.tabs-sidebar a').removeClass("on");
                                                $(this).addClass("on")
                                                tab = $(this).data('tab');
                                                $('.tab').removeClass('on');
                                                $('.tab-'+tab).addClass('on');
                                                $('.tabs').animate({height:$('.tab-'+tab).outerHeight(true)}, 500)

                                        })
                                        //console.log($('.tab.on').outerHeight(true))
                                        $('.tabs').animate({height:$('.tab.on').outerHeight(true)}, 500)

                                }

                                $('.top-menu').on('touchstart touchmove', function (e) {
                                        e.stopPropagation();
                                });

                                executeSizeFunction();
                        });

                        $(window).resize(function(){
                                console.log("resize")
                                setMenuHeight();
                                resizeElements();
                                executeSizeFunction();
                        })
