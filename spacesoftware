  $( document ).ready(function() { console.log('fullscreen');
  var wheight = $(window).height() + 20;

  if ($(window).width() > 768){
  $(".fullscreen").each(function(){

  var cheight = $(this).height();

  //$(this).height(Math.max(cheight,wheight));
  $(this).css('min-height', String(Math.max(cheight,wheight))+'px');
  });


